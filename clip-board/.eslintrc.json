{
  "parser": "@typescript-eslint/parser",
  "extends": ["next/core-web-vitals", "next/typescript", "next", "prettier"],
  "rules": {
    "eqeqeq": "error",
    "semi": ["error", "always"],
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
